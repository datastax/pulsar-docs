{
  "name": "luna-streaming",
  "scripts": {
    "build:prod:luna-streaming": "npm run build:publish:luna-streaming",
    "build:dev:luna-streaming": "npm run build:local:luna-streaming",
    "build:publish:luna-streaming": "antora --log-level error --fetch --clean playbooks/site-publish-luna-streaming.yaml",
    "build:local:luna-streaming": "antora --stacktrace --fetch --clean playbooks/site-local-luna-streaming.yaml",
    "build:linkinator:luna-streaming": "npx linkinator https://docs.datastax.com/en/luna-streaming/docs/ --recurse --skip ' https://localhost?'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datastax/pulsar-docs.git"
  },
  "dependencies": {
    "@antora/cli": "~3.0.1",
    "@antora/site-generator-default": "~3.0.1",
    "linkinator": "~3.0.3",
    "async": "~3.2.4",
    "mobx": "~6.0.4",
    "react": "~16.8.4",
    "react-dom": "~16.8.4",
    "rxjs": "~7.0.1",
    "styled-components": "~5.1.1"
  }
}
